{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "new-cap": 2,
    "no-console": 1,
    "no-underscore-dangle": 2,
    "no-use-before-define": 2,
    "no-dupe-keys": 0,
    "quotes": [
      1,
      "single"
    ],
    "react/require-render-return": 2,
    "react/jsx-uses-react": 2,   
    "react/jsx-uses-vars": 2
  }
}
